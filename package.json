{
  "name": "android-apks",
  "version": "1.0.0",
  "description": "Download Android apks",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "repository": "k03mad/android-apks",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=21"
  },
  "dependencies": {
    "@k03mad/request": "4.0.0",
    "chalk": "5.3.0",
    "compression": "1.7.4",
    "debug": "4.3.4",
    "express": "4.18.2",
    "express-handlebars": "7.1.2",
    "express-minify": "1.0.0",
    "handlebars": "4.7.8",
    "handlebars-helper-minify": "0.1.3",
    "handlebars-helpers": "0.10.0",
    "helmet": "7.1.0",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "p-map": "6.0.0",
    "pretty-bytes": "6.1.1",
    "recron": "1.1.5"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "13.3.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "eslint": "8.54.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "nodemon": "3.0.2"
  },
  "scripts": {
    "lint": "eslint ./ --report-unused-disable-directives",
    "lint:github": "eslint ./ --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "clean": "rm -rfv ./node_modules || true",
    "setup": "npm run clean && npm i",
    "setup:prod": "npm run clean && npm i --omit=dev",
    "start": "node index",
    "start:dev": "DEBUG=mad* nodemon -e js,json,css,hbs,woff,woff2,ico,webmanifest index",
    "apk:get": "DEBUG=mad* node app/cron/scripts/run",
    "apk:dl": "DEBUG=mad* TYPE=dl node app/cron/scripts/run",
    "prepare": "husky install || true"
  }
}
