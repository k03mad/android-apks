{
  "name": "android-apks",
  "version": "1.0.0",
  "description": "Download Android apks",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "repository": "k03mad/android-apks",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=21"
  },
  "dependencies": {
    "@k03mad/request": "4.2.0",
    "chalk": "5.3.0",
    "cheerio": "1.0.0-rc.12",
    "compression": "1.7.4",
    "debug": "4.3.4",
    "express": "4.18.2",
    "express-handlebars": "7.1.2",
    "express-minify": "1.0.0",
    "helmet": "7.1.0",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "p-map": "7.0.0",
    "prom-client": "15.1.0",
    "recron": "1.1.5"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "15.2.0",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@stylistic/eslint-plugin": "1.5.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "46.9.1",
    "eslint-plugin-n": "16.5.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "50.0.1",
    "husky": "8.0.3",
    "nodemon": "3.0.2",
    "stylelint": "16.0.2",
    "stylelint-config-standard": "35.0.0",
    "stylelint-sarif-formatter": "1.0.7"
  },
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint ./ --report-unused-disable-directives",
    "lint:eslint:github": "eslint ./ --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "lint:stylelint": "stylelint '**/*.css'",
    "lint:stylelint:github": "stylelint '**/*.css' --custom-formatter=stylelint-sarif-formatter -o stylelint-results.sarif",
    "clean": "rm -rf ./node_modules || true",
    "setup": "npm run clean && npm i",
    "setup:prod": "npm run clean && npm i --omit=dev",
    "start": "node index",
    "start:dev": "DEBUG=mad* nodemon -e js,json,css,hbs,woff,woff2,ico,webmanifest index",
    "apk:get": "DEBUG=mad* node app/cron/scripts/run",
    "apk:dl": "DEBUG=mad* TYPE=dl node app/cron/scripts/run",
    "prepare": "husky install || true"
  }
}
