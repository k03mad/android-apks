{
  "name": "android-apks",
  "version": "1.0.0",
  "description": "Download Android apks",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "repository": "k03mad/android-apks",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=21"
  },
  "dependencies": {
    "@k03mad/request": "5.3.0",
    "chalk": "5.3.0",
    "cheerio": "1.0.0-rc.12",
    "compression": "1.7.4",
    "debug": "4.3.4",
    "express": "4.18.2",
    "express-handlebars": "7.1.2",
    "express-minify": "1.0.0",
    "helmet": "7.1.0",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "p-map": "7.0.1",
    "prom-client": "15.1.0",
    "recron": "1.1.5"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "17.13.0",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "nodemon": "3.0.2",
    "stylelint": "16.1.0",
    "stylelint-config-standard": "36.0.0"
  },
  "scripts": {
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
    "lint:eslint": "eslint ./ --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "clean": "pnpm run clean:modules && pnpm run clean:eslint:cache && pnpm run clean:stylelint:cache",
    "clean:modules": "rm -rf ./node_modules || true",
    "clean:eslint:cache": "rm -rf .eslintcache || true",
    "clean:stylelint:cache": "rm -rf .stylelintcache || true",
    "setup": "pnpm run clean && pnpm i",
    "setup:prod": "pnpm run clean && pnpm i --prod",
    "start": "node app/index",
    "start:dev": "DEBUG=mad* nodemon -e js,json,css,hbs,woff,woff2,ico,webmanifest app/index",
    "apk:get": "DEBUG=mad* node app/cron/scripts/run",
    "apk:dl": "DEBUG=mad* TYPE=dl node app/cron/scripts/run",
    "prepare": "husky install || true"
  }
}
